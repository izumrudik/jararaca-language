{
    "name": "jararaca-language",
    "displayName": "jararaca-language",
    "description": "Jararaca language support for Visual Studio Code.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jararaca",
            "aliases": ["jararaca"],
            "extensions": [".ja"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jararaca",
            "scopeName": "source.jararaca",
            "path": "./syntaxes/jararaca.tmLanguage.json"
        }]
    }
}